<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://code.alibabatech.com/schema/dubbo
            http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="pin-id-service" logger="slf4j"/>

    <dubbo:module name="pin-id" />

    <dubbo:registry
            protocol="${dubbo.registry.protocol}"
            address="${dubbo.registry.address}"
            check="${dubbo.registry.check}"
            timeout="${dubbo.registry.timeout}"
            file="${dubbo.registry.file}"
    />

    <dubbo:protocol name="${dubbo.protocol.name}"
                    host="${dubbo.protocol.host}"
                    port="${dubbo.protocol.port}"
                    threads="${dubbo.protocol.threads}"
    />

    <dubbo:provider timeout="${dubbo.provider.timeout}" />
    <!--<dubbo:monitor protocol="registry" />-->

    <dubbo:service interface="com.pin.idgen.rpc.api.IdGenRpcService" ref="idGenRpc" retries="0"/>
    <dubbo:service interface="com.pin.idgen.rpc.api.SequenceRpcService" ref="sequenceRpc" retries="0"/>
</beans>
